<project
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"
	xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>tools.dynamia.cms</groupId>
		<artifactId>tools.dynamia.cms.parent</artifactId>
		<version>3.0.0.RC</version>
	</parent>
	<name>DynamiaCMS - Products Admin (${project.version})</name>
	<groupId>tools.dynamia.cms</groupId>
	<artifactId>tools.dynamia.cms.products.admin</artifactId>
	<version>3.0.0.RC</version>

	<url>http://maven.apache.org</url>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
					<encoding>${project.build.sourceEncoding}</encoding>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>

		<dependency>
			<groupId>tools.dynamia.cms</groupId>
			<artifactId>tools.dynamia.cms.products</artifactId>
			<version>3.0.0.RC</version>
		</dependency>
		<dependency>
			<groupId>tools.dynamia.cms</groupId>
			<artifactId>tools.dynamia.cms.admin.ui</artifactId>
			<version>3.0.0.RC</version>
		</dependency>
		<dependency>
			<groupId>tools.dynamia.cms</groupId>
			<artifactId>tools.dynamia.cms.importer.admin</artifactId>
			<version>3.0.0.RC</version>
		</dependency>
	</dependencies>
</project>
